{
  "name": "todo-form",
  "version": "0.0.1",
  "description": "todo-form",
  "productName": "todo-form",
  "author": "Antony <antony.raj@pickles.com.au>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "serve": "quasar serve ./dist/spa --port 5001 --cache 0 --cors",
    "build:watch": "nodemon",
    "watch:dev": "concurrently \"pnpm build:watch\" \"pnpm serve\""
  },
  "dependencies": {
    "pinia": "^3.0.1",
    "@quasar/extras": "^1.16.4",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}